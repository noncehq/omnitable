{
	"name": "appframe",
	"author": "1yasa",
	"version": "0.0.0",
	"description": "Preset components and utils for omnitable app.",
	"scripts": {
		"build": "cross-env NODE_ENV=production rslib build && pnpm run build:theme && pnpm run build:styles && tsc -p ./tsconfig.build.json",
		"build:theme": "bun ./build/theme",
		"build:styles": "bun ./build/styles"
	},
	"exports": {
		"./components": {
			"types": "./dist/components/index.d.ts",
			"import": "./dist/components/index.js",
			"default": "./dist/components/index.js"
		},
		"./theme": {
			"types": "./dist/theme/index.d.ts",
			"import": "./dist/theme/index.js",
			"default": "./dist/theme/index.js"
		},
		"./preset": {
			"types": "./dist/preset/index.d.ts",
			"import": "./dist/preset/index.js",
			"default": "./dist/preset/index.js"
		},
		"./init.css": {
			"import": "./dist/init.css",
			"default": "./dist/init.css"
		},
		"./global.css": {
			"import": "./dist/global.css",
			"default": "./dist/global.css"
		}
	},
	"dependencies": {
		"@abraham/reflection": "^0.13.0",
		"@ant-design/v5-patch-for-react-19": "^1.0.3",
		"@phosphor-icons/react": "2.1.7",
		"@swc/helpers": "^0.5.17",
		"ahooks": "^3.8.4",
		"antd": "^5.25.1",
		"classix": "^2.2.2",
		"dayjs": "^1.11.13",
		"fast-equals": "^5.2.2",
		"lodash-es": "^4.17.21",
		"lucide-react": "^0.509.0",
		"mobx": "^6.13.7",
		"mobx-react-lite": "^4.1.0",
		"motion": "^12.11.0",
		"ofetch": "^1.4.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-error-boundary": "^6.0.0",
		"stk": "workspace:*",
		"tslib": "^2.8.1",
		"tsyringe": "^4.10.0"
	},
	"devDependencies": {
		"@rsbuild/plugin-react": "^1.3.1",
		"@rslib/core": "^0.6.9",
		"@svgr/webpack": "^8.1.0",
		"@types/dom-view-transitions": "^1.0.6",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.15.17",
		"@types/react": "^19.1.3",
		"@types/react-dom": "^19.1.3",
		"autoprefixer": "^10.4.21",
		"bun": "^1.2.13",
		"bun-types": "^1.2.13",
		"cross-env": "^7.0.3",
		"cssnano": "^7.0.7",
		"deepmerge-ts": "^7.1.5",
		"postcss": "^8.5.3",
		"postcss-calc": "^10.1.1",
		"postcss-import": "^16.1.0",
		"postcss-loader": "^8.1.1",
		"postcss-nested": "^7.0.2",
		"react-refresh": "^0.17.0",
		"tinyglobby": "^0.2.13",
		"typescript": "^5.8.3"
	}
}
